{
	"preset": "ts-jest",
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest",
		"^.+\\.(js|jsx)$": "babel-jest"
  },
  "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "testEnvironment": "node",
	"transformIgnorePatterns": [
		"node_modules/(?!variables/.*)"
	]
}